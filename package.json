{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "export VITE_CJS_IGNORE_WARNING=true; vitest --config config/vitest.config.ts",
    "test:cov": "export VITE_CJS_IGNORE_WARNING=true; vitest run --config config/vitest.config.ts --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "export VITE_CJS_IGNORE_WARNING=true; vitest run --config config/vitest.config.ts",
    "db:generate": "npx drizzle-kit generate --config config/drizzle.config.ts",
    "db:push": "npx drizzle-kit push --config config/drizzle.config.ts"
  },
  "dependencies": {
    "@fastify/multipart": "^9.0.1",
    "@fastify/static": "^7.0.4",
    "@fastify/swagger": "^6.0.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/platform-fastify": "^10.4.6",
    "@nestjs/swagger": "^8.0.1",
    "@ts-rest/core": "^3.51.0",
    "@ts-rest/nest": "^3.51.0",
    "@ts-rest/open-api": "^3.51.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "argon2": "^0.41.1",
    "drizzle-orm": "^0.36.0",
    "drizzle-zod": "^0.5.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "luxon": "^3.5.0",
    "nestjs-drizzle": "^0.9.7",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.42",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.8.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.1.4",
    "drizzle-kit": "^0.27.0",
    "eslint": "^9.13.0",
    "eslint-config-nestjs": "^0.8.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
